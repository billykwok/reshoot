{
  "name": "@reshoot/loader",
  "description": "Webpack loader integration for Reshoot",
  "version": "2.1.0",
  "type": "module",
  "exports": {
    "import": "./lib/es/index.mjs",
    "require": "./lib/index.cjs",
    "types": "./types/index.d.ts"
  },
  "module": "./lib/es/index.mjs",
  "main": "./lib/index.cjs",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "engines": {
    "node": ">=13.0.0"
  },
  "author": "Billy Kwok <opensource@billykwok.me>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "responsive",
    "image",
    "webpack",
    "loader"
  ],
  "scripts": {
    "clean": "rm -rf lib types",
    "lint": "eslint ./src ./test",
    "build": "rollup -c rollup.config.js && tsc --project tsconfig.types.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules pnpm jest"
  },
  "dependencies": {
    "@reshoot/core": "workspace:^2.1.0",
    "@reshoot/types": "workspace:^2.0.0",
    "@babel/runtime-corejs3": "^7",
    "json5": "^2.2.1",
    "loader-utils": "^3.2.0"
  },
  "peerDependencies": {
    "webpack": "^4 || ^5"
  },
  "devDependencies": {
    "@types/loader-utils": "^2.0.3",
    "memfs": "^3.4.6",
    "webpack": "^5.73.0"
  }
}
