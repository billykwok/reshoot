{
  "name": "@reshoot/remark-mdx-image",
  "description": "MDX integration for Reshoot",
  "version": "1.8.1",
  "type": "module",
  "exports": {
    "import": "./lib/es/index.mjs",
    "require": "./lib/index.cjs",
    "types": "./types/index.d.ts"
  },
  "module": "./lib/es/index.mjs",
  "main": "./lib/index.cjs",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "author": "Billy Kwok <opensource@billykwok.me>",
