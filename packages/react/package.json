{
  "name": "@reshoot/react",
  "description": "All-in-one responsive image processing library with webpage loader, babel macro and React component",
  "version": "1.2.6",
  "type": "commonjs",
  "exports": {
    "import": "./lib/es/index.js",
    "require": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Billy Kwok <opensource@billykwok.me>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "srcset",
    "image",
    "img",
    "react",
    "responsive",
    "babel"
  ],
  "repository": "https://github.com/billykwok/reshoot",
  "bugs": {
    "url": "https://github.com/billykwok/reshoot/issues"
  },
  "homepage": "https://github.com/billykwok/reshoot/tree/master/packages/react",
  "scripts": {
    "lint": "eslint ./src/**",
    "clean": "rm -rf lib types",
    "build": "yarn clean && rollup -c rollup.config.js && rollup -c rollup.config.js --format es --dir ./lib/es && tsc --project tsconfig.json"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.11.0",
    "object-assign": "4.1.1",
    "react": "^16.13.1"
  }
}
