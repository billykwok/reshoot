{
  "name": "@reshoot/react",
  "description": "React integration for Reshoot",
  "version": "2.0.2",
  "type": "module",
  "exports": {
    "./styles.css": {
      "import": "./lib/es/styles.css",
      "require": "./lib/styles.css"
    },
    ".": {
      "import": "./lib/es/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./types/index.d.ts"
    }
  },
  "module": "./lib/es/index.mjs",
  "main": "./lib/index.cjs",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": ">=13.0.0"
  },
  "author": "Billy Kwok <opensource@billykwok.me>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "responsive",
    "image",
    "react",
    "react-dom"
  ],
  "scripts": {
    "clean": "rm -rf lib types",
    "lint": "eslint ./src ./test",
    "build": "rollup -c rollup.config.js && tsc --project tsconfig.types.json",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7",
    "@reshoot/types": "workspace:^2.0.1",
    "react": "^17 || ^18",
    "use-lifecycle-ref": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-react": "^7.17.12",
    "@linaria/babel-preset": "^3.0.0-beta.22",
    "@linaria/core": "^3.0.0-beta.22",
    "@linaria/rollup": "^3.0.0-beta.22",
    "@linaria/shaker": "^3.0.0-beta.22",
    "@testing-library/react": "^13.3.0",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "eslint": "^8.18.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-environment-jsdom": "^28.1.1",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
