{
  "name": "@reshoot/macro",
  "description": "All-in-one responsive image processing library with webpage loader, babel macro and React component",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Billy Kwok <opensource@billykwok.me>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "srcset",
    "image",
    "img",
    "responsive",
    "babel",
    "babel-macros",
    "babel-plugin-macros"
  ],
  "repository": "https://github.com/billykwok/reshoot",
  "bugs": {
    "url": "https://github.com/billykwok/reshoot/issues"
  },
  "homepage": "https://github.com/billykwok/reshoot/tree/master/packages/macro",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && babel src --root-mode=upward --out-dir lib --extensions .ts && tsc --project tsconfig.json",
    "lint": "eslint ./src/**"
  },
  "peerDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/types": "^7.10.5",
    "babel-plugin-macros": "^2.8.0"
  }
}
