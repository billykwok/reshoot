{
  "name": "@reshoot/macro",
  "description": "Babel macro integration for Reshoot",
  "version": "2.1.4",
  "type": "module",
  "exports": {
    "import": "./lib/es/index.mjs",
    "require": "./lib/index.cjs",
    "types": "./types/index.d.ts"
  },
  "module": "./lib/es/index.mjs",
  "main": "./lib/index.cjs",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=13.0.0"
  },
  "author": "Billy Kwok <opensource@billykwok.me>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "responsive",
    "image",
    "babel",
    "macro"
  ],
  "scripts": {
    "clean": "rm -rf lib types",
    "lint": "eslint ./src ./test",
    "build": "rollup -c rollup.config.js && tsc --project tsconfig.types.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules pnpm jest"
  },
  "dependencies": {
    "@reshoot/types": "workspace:^2.0.3",
    "@babel/core": "^7",
    "@babel/template": "^7",
    "@babel/types": "^7"
  },
  "peerDependencies": {
    "@reshoot/loader": "workspace:^2.1.3",
    "babel-plugin-macros": "^3"
  },
  "devDependencies": {
    "@babel/template": "^7.16.7",
    "@babel/types": "^7.18.4",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/babel__core": "^7.1.19",
    "@types/babel__template": "^7.4.1",
    "babel-plugin-macros": "^3.1.0"
  }
}
