{
  "name": "@reshoot/macro",
  "description": "All-in-one responsive image processing library with webpage loader, babel macro and React component",
  "version": "0.3.12",
  "main": "lib/index.js",
  "author": "Billy Kwok <xobkwok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "reshoot",
    "srcset",
    "image",
    "img",
    "responsive",
    "babel",
    "babel-macros",
    "babel-plugin-macros"
  ],
  "files": [
    "lib",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/billykwok/reshoot/packages/reshoot-macro",
  "bugs": {
    "url": "https://github.com/billykwok/reshoot/issues"
  },
  "homepage": "https://github.com/billykwok/reshoot",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && babel src --root-mode=upward --out-dir lib --extensions .ts,.tsx,.mjs,.js,.jsx",
    "eslint": "eslint ./src/**",
    "dtslint": "dtslint --onlyTestTsNext --localTs ../../node_modules/typescript/lib types",
    "lint": "yarn eslint && yarn dtslint"
  },
  "peerDependencies": {
    "babel-plugin-macros": "^2.6.1"
  }
}
