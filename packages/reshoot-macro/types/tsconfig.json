{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext", "dom", "dom.iterable"],
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": { "@reshoot/macro": ["."] }
  }
}
