  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules/.cache/@reshoot",
    "build": "lerna run --parallel --no-private build",
    "lint": "lerna run --parallel --no-private lint",
    "test": "yarn clean && yarn workspace @reshoot/macro run build && jest --maxWorkers=4",
