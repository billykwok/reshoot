{
  "name": "reshoot",
  "description": "All-in-one responsive image processing library with webpage loader, babel macro and React component",
  "author": "Billy Kwok <xobkwok@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "lerna run --parallel --no-private build",
    "eslint": "lerna run --parallel --no-private eslint",
    "dtslint": "lerna run --parallel --no-private dtslint",
    "lint": "lerna run --parallel --no-private lint",
    "test:package": "jest --detectOpenHandles",
    "test:snapshot": "yarn build && webpack --config=integration/webpack.config.js && jest integration --detectOpenHandles",
    "test": "yarn build && webpack --config=integration/webpack.config.js && yarn test:package",
    "commit": "commit",
    "release": "lerna publish --yes --no-changelog"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "eslint-utils": "^1.4.2",
    "lodash": "^4.17.15",
    "lodash.template": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "mixin-deep": "^1.3.2",
    "serialize-javascript": "^2.1.1",
    "set-value": "^2.0.1"
