{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [
    ".env",
    "rollup.config.js",
    "babel.config.cjs",
    "tsconfig.json",
    "tsconfig.types.json",
    "jest.config.js",
    "$GITHUB_TOKEN"
  ],
  "pipeline": {
    "clean": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["clean", "^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["lib/**", "dist/**", "types/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx",
        "test/**/*.snap"
      ]
    },
    "lint": {
      "outputs": []
    }
  }
}
